            <items>
                <item>
                    <name>Cpanel - Number of mails in queue</name>
                    <key>mailqueue-exim</key>
                    <delay>60s</delay>
                    <applications>
                        <application>
                            <name>Cpanel</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{min(60)}&gt;=100</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(60)}&lt;100</recovery_expression>
                            <name>Number of mail in queue is high on {HOST.NAME} ({ITEM.LASTVALUE} mails)</name>
                            <priority>WARNING</priority>
                        </trigger>
                        <trigger>
                            <expression>{min(60)}&gt;=150</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(60)}&lt;150</recovery_expression>
                            <name>Number of mail in queue is too high on {HOST.NAME} ({ITEM.LASTVALUE} mails)</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
